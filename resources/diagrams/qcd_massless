
%  simplified QCD with neither flavour nor colour indices


%  propagators

  [ q, qB, - ; pfunct='fprop', m='0']
  [ top, topB, - ; pfunct='fprop', m='0']
  [ g, g, +; pfunct='gprop', m='0' ]
  [ gh, ghB, -; pfunct='ghprop', m='0' ]



%  vertices

  [ qB, q, g ]
  [ topB, top, g ]
  [ g, g, g ]
  [ g, g, g, g ]
  [ ghB, gh, g ]

