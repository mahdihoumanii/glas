     (1 - (2*ep)/3)}, BL[top1, {1, -1, 1, 0}] -> {-1/2*s12}, 
  BL[top1, {1, 0, 1, -1}] -> {1 + s12/2}, BL[top1, {1, 0, 1, 0}] -> {1}}}
